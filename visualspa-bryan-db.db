CREATE TABLE hospede (
    nome VARCHAR (25) NOT NULL,
	genero VARCHAR (1),
	biotipo VARCHAR (1),
	altura NUMERIC (5,2),
	PRIMARY KEY (nome)	
)
	
CREATE TABLE quarto (
	nome VARCHAR (25) NOT NULL,
	quarto INT NOT NULL,
	chegada DATE NOT NULL,
	saida DATE,
	desconto NUMERIC (5,2),
	PRIMARY KEY (nome, quarto)
)